<Window x:Class="HifiPrototype2.View.SelectTemplateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HifiPrototype2.View"
        mc:Ignorable="d"
        Title="Vælg Grundplan" Height="300" Width="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <ListBox Grid.Row="0" Margin="3" Padding="3" x:Name="TemplateScheduleListBox"/>
        <StackPanel Grid.Row="1" Orientation="Horizontal" Margin="3" HorizontalAlignment="Right">
            <Button  Margin="3" Padding="2" x:Name="Cancel" Content="Anuller" Click="Cancel_Click"/>
            <Button Margin="3" Padding="2" x:Name="Load" Content="Indlæs" Click="Load_Click"/>
        </StackPanel>
    </Grid>
</Window>
